generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Float
  description String?
  image       String?
  createdAt   DateTime @default(now())
}

model Blog {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  createdAt DateTime @default(now())
}
